    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.szxb.wxx.MainActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:textSize="50sp"
        android:singleLine="false"
        android:textStyle="bold"
        android:text="
- regn rytmen våren kvar en hel mening, fortfarande utan att ge upp, med en ganska till säsonger i sommar.regn, regn, alltid har en vidsträckt himmel, vilket gör att det är cool ytlig sommaren under regn och dimma - - går långsamt.
"
        android:id="@+id/textViewHide"/>
    <Button
        android:id="@+id/print"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PRINT" />
</RelativeLayout>
